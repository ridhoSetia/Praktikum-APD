    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="hargaBerasMawar, hargaBerasSani, hargaBerasMaknyus, hargaBeras, inputNIM" type="Integer" array="False" size=""/>
            <declare name="inputNama" type="String" array="False" size=""/>
            <output expression="&quot;Masukkan nama lengkap&quot;" newline="True"/>
            <input variable="inputNama"/>
            <output expression="&quot;Masukkan NIM&quot;" newline="True"/>
            <input variable="inputNIM"/>
            <output expression="&quot;Masukkan Harga beras&quot;" newline="True"/>
            <input variable="hargaBeras"/>
            <assign variable="hargaBerasMawar" expression="hargaBeras - (0.11 * hargaBeras)"/>
            <assign variable="hargaBerasSani" expression="hargaBeras - (0.14 * hargaBeras)"/>
            <assign variable="hargaBerasMaknyus" expression="hargaBeras - (0.17 * hargaBeras)"/>
            <output expression="inputNama &amp; &quot; dengan NIM &quot; &amp; inputNIM &amp; &quot; ingin membeli beras seharga Rp&quot; &amp; hargaBeras &amp; &quot;.&quot;" newline="True"/>
            <output expression="&quot; Jika dia membeli beras Mawar ia harus membayar Rp&quot; &amp; hargaBerasMawar &amp; &quot; setelah mendapat diskon 11%.&quot;" newline="True"/>
            <output expression="&quot; Jika dia membeli beras Sani ia harus membayar Rp&quot; &amp; hargaBerasSani &amp; &quot; setelah mendapat diskon 14%.&quot;" newline="True"/>
            <output expression="&quot; Jika dia membeli beras Maknyus ia harus membayar Rp&quot; &amp; hargaBerasMaknyus &amp; &quot; setelah mendapat diskon 17%.&quot;" newline="True"/>
        </body>
    </function>
